<h1>  <%= pluralize(@users.size, "User") %> users listed.</h1>
<ul class="users">
    <% @users.each do |user| %>
      <li>
        <%= link_to user.name, user  %>
        created
        <%= time_ago_in_words(user.created_at) %> ago
        <h3>Member Since</h3>
          <p>
            <%= user.created_at.strftime("%B %Y") %>
          </p>
      </li>
    <% end %>
</ul>
